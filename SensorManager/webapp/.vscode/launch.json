{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run SensorManager",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/SensorManager/node_modules/@sap/html5-repo-mock/index.js",
      "cwd": "${workspaceFolder}/SensorManager",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_flat_run_config_handler_id\",\"runnableId\":\"/home/user/projects/TechEd2020/SensorManager\"}"
      },
      "args": [
        "--standalone",
        "/keepcoolSensorManager/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "envFile": "${workspaceFolder}/SensorManager/.env1",
      "preLaunchTask": "Build SensorManager"
    }
  ]
}